{
    "manifest_version": 3,
    "name": "Password Generator",
    "description": "Generate strong, memorable passwords",
    "version": "1.0.1",
    "permissions": ["activeTab"],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "images/128.png",
            "48": "images/128.png",
            "128": "images/128.png"
        }
    },
    "icons": {
        "16": "images/128.png",
        "48": "images/128.png",
        "128": "images/128.png"
    }
}
